#include <stdio.h>
int main (void)

{
    int a;
    int b;
    int c;
    
    printf("Please enter a number:\n");
    scanf("%d", &a);
    printf("Please enter another number:\n");
    scanf("%d", &b);
    printf("Please enter a third number:\n\n");
    scanf("%d", &c);
   
    int biggest = a;
    int smallest = a;
    
    if(b > biggest)
    {
        biggest = b;
    }
    
    if (c > biggest) 
    {
    biggest = c;
    }
    
   if (b < smallest) {
        smallest = b;
    }
    if (c < smallest) {
        smallest = c;
    }    
    
    printf("Biggest number was: %d\n", biggest);
    printf("Smallest number was: %d\n", smallest);    

}
